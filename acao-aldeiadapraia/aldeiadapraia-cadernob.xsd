									<xs:enumeration value="Trabalho" />
									<xs:enumeration value="Transporte" />
									<xs:enumeration value="Doença" />
									<xs:enumeration value="Gravidez" />
									<xs:enumeration value="Viagem" />
									<xs:enumeration value="Faltas" />

									<xs:enumeration value="Suspensão" />
									<xs:enumeration value="Não desejava continuar" />
									<xs:enumeration value="Outros" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
