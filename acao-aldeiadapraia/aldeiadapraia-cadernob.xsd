									<xs:element name="alcoolismo" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Alcoolismo</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="naosei" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Não sei</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="naoTem" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Não tem</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
