									</xs:element>
									<xs:element name="nis" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>NIS</xhtml:label>

											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="naotem" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Não tem</xhtml:label>
											</xs:appinfo>

										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="rg" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>7 - Número do RG</xhtml:label>

