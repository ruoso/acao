									<xhtml:label>8 - Órgão Expedidor</xhtml:label>
								</xs:appinfo>

							</xs:annotation>
						</xs:element>
						<xs:element name="cpf" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>9 - CPF</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

						</xs:element>
						<xs:element name="nis" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>10 - NIS</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="data" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>11 - Data Nascimento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="idade" type="xs:integer" minOccurs="0">

							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>12 - Idade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="situacaoConjugal" minOccurs="0">
							<xs:annotation>

