											</xs:appinfo>

										</xs:annotation>
									</xs:element>
									<xs:element name="outras" type="xs:string"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Outras</xhtml:label>
											</xs:appinfo>
										</xs:annotation>

									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="saude">
				<xs:annotation>
