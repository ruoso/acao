						<xs:element name="nome" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2 - Nome</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="sexo" minOccurs="0">

							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>3 - Sexo</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
