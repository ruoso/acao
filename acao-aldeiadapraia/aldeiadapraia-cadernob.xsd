												<xhtml:label>Futebol/Outros Esportes</xhtml:label>

											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="sairComVizinhos" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Sair com amigos/Vizinhos</xhtml:label>
											</xs:appinfo>

										</xs:annotation>
									</xs:element>
									<xs:element name="assistirTV" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Assistir TV</xhtml:label>
											</xs:appinfo>
										</xs:annotation>

									</xs:element>
									<xs:element name="escultarMusica" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Escutar Música</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="festas" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Festas</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="participarDeAlumaOrganizacao"
										type="xs:boolean">

										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Participar de alguma organização</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="frequentarBares" type="xs:boolean">
										<xs:annotation>

											<xs:appinfo>
												<xhtml:label>Frequentar Bares</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="vaiaPracas" type="xs:boolean">
