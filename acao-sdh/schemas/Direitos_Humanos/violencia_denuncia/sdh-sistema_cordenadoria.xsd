<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://schemas.fortaleza.ce.gov.br/acao/sdh-sistema_cordenadoria.xsd"
    xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/sdh-sistema_cordenadoria.xsd"
    xmlns:idx="http://schemas.fortaleza.ce.gov.br/acao/indexhint.xsd"
    xmlns:xsdext="http://schemas.fortaleza.ce.gov.br/xsdform/extensions.xsd"
	elementFormDefault="qualified">

	<xs:element name="sistemacordenadoria" type="sistemacordenadoria">
		<xs:annotation>
			<xs:appinfo>
				<xhtml:label>SISTEMA COORDENADORIA</xhtml:label>
                <class:classificacoes>
                    <class:classificacao>cn=Relatórios,cn=Direitos Humanos</class:classificacao>
                </class:classificacoes>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="sistemacordenadoria">
		<xs:sequence>

			<xs:element name="sistemaCordenadoria">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1. O que produziu/ realizou? </xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
                         <xs:element name="nome" minOccurs="0"  >
			                <xs:annotation>
				                <xs:appinfo>
					                <xhtml:label>Nome</xhtml:label>
				                </xs:appinfo>
			                </xs:annotation>
			                <xs:simpleType>
					    <xs:restriction base="xs:string">
						    <xs:maxLength value="255" />
					    </xs:restriction>
				        </xs:simpleType>
                        </xs:element> 
						<xs:element name="curso" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Curso</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="seminario" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Seminário</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="oficina" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Oficina</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="simposio" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Simpósio</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="encontro" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Encontro</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="padreGrio" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Padê Griô</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="rodaDeConversa" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Roda de Conversa</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="paradaPelaDiversidadeSexual" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label> Parada pela Diversidade Sexual</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="cortejo" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Cortejo</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="caminhada" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Caminhada</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="atoShow" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Ato Show</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="quartaCultural" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quarta Cultural</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="tercaNegra" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Terça Negra</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="multiraoDeSubregistro" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Mutirão de Subregistro</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="multiraoDeUniaoEstavel" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Mutirão de União Estável</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>	
						<xs:element name="audienciaPublica" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Audiência Pública</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>	
						<xs:element name="coferencia" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Coferência</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="planoMunicipalDaIgualdadeRacial" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Plano Municipal da Igualdade Racial</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>	
						<xs:element name="planoMunicipalDeEducacaoDireitosHumanos" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Plano Municipal de Educação em Direitos Humanos</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>	
					    <xs:element name="forum" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Fórum</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>							
                        <xs:element name="acoesComunitarias" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Ações Comunitárias</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>																																							
						<xs:element name="outros" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Outros</xhtml:label>
								 </xs:appinfo>
							</xs:annotation>
						</xs:element>						
			<xs:element name="especificarOutros" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Especifique Outros</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>


  
                      <xs:element name="dataEvento" minOccurs="0"  type="xs:date">
			                <xs:annotation>
				                <xs:appinfo>
					                <xhtml:label>2.0 Data do Evento</xhtml:label>
				                </xs:appinfo>
			                </xs:annotation>

                        </xs:element>
                        
                        <xs:element name="hora" minOccurs="0"  >
			                <xs:annotation>
				                <xs:appinfo>
					                <xhtml:label>3.0 Hora</xhtml:label>
				                </xs:appinfo>
			                </xs:annotation>
			                <xs:simpleType>
					    <xs:restriction base="xs:string">
						    <xs:maxLength value="255" />
					    </xs:restriction>
				        </xs:simpleType>
                        </xs:element>
                        
						<xs:element  name="informacao"> 
						        <xs:annotation>          	
								   <xs:appinfo>
									<xhtml:label>4.0 Local do Evento</xhtml:label>
								   </xs:appinfo>
								 </xs:annotation>
						    <xs:complexType>
					           <xs:sequence>
			              	   </xs:sequence>
						    </xs:complexType>		
						</xs:element>
                        
                         <xs:element name="endereco" minOccurs="0"  >
			                <xs:annotation>
				                <xs:appinfo>
					                <xhtml:label>Endereço</xhtml:label>
				                </xs:appinfo>
			                </xs:annotation>
			                <xs:simpleType>
					    <xs:restriction base="xs:string">
						    <xs:maxLength value="255" />
					    </xs:restriction>
				        </xs:simpleType>
                        </xs:element>     
                        
						<xs:element name="bairro" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Bairro</xhtml:label>
								</xs:appinfo>
								<xsdext:extensions>
									<xsdext:engine>autoComplete</xsdext:engine>
									<xsdext:service>/bairros</xsdext:service>
								</xsdext:extensions>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element> 
                        
                       <xs:element name="regional" minOccurs="0"  >
			                <xs:annotation>
				                <xs:appinfo>
					                <xhtml:label>Regional</xhtml:label>
				                </xs:appinfo>
			                </xs:annotation>
			                <xs:simpleType>
					    <xs:restriction base="xs:string">
						    <xs:maxLength value="255" />
					    </xs:restriction>
				        </xs:simpleType>
                        </xs:element>    
                        
                       <xs:element name="publicoAfetado">
	            <xs:annotation>
		            <xs:appinfo>
			            <xhtml:label>5.0 Público Afetado</xhtml:label>
		            </xs:appinfo>
	            </xs:annotation>
	            <xs:complexType>
		            <xs:sequence>
		                   <xs:element name="ate50Pessoas" type="xs:boolean">
							    <xs:annotation>
								    <xs:appinfo>
									    <xhtml:label>Até 50 Pessoas</xhtml:label>
								    </xs:appinfo>
							    </xs:annotation>
						    </xs:element>
		                   <xs:element name="entre50e100Pessoas" type="xs:boolean">
							    <xs:annotation>
								    <xs:appinfo>
									    <xhtml:label>Entre 51 e 100 Pessoas</xhtml:label>
								    </xs:appinfo>
							    </xs:annotation>
						    </xs:element>
		                   <xs:element name="entre101e200Pessoas" type="xs:boolean">
							    <xs:annotation>
								    <xs:appinfo>
									    <xhtml:label>Entre 101 e 200 Pessoas</xhtml:label>
								    </xs:appinfo>
							    </xs:annotation>
						    </xs:element>
						    
						    <xs:element name="entre201e300Pessoas" type="xs:boolean">
							    <xs:annotation>
								    <xs:appinfo>
									    <xhtml:label>Entre 201 e 300 Pessoas</xhtml:label>
								    </xs:appinfo>
							    </xs:annotation>
						    </xs:element>
						    <xs:element name="entre201e300Pessoas" type="xs:boolean">
							    <xs:annotation>
								    <xs:appinfo>
									    <xhtml:label>Entre 301 e 400 Pessoas</xhtml:label>
								    </xs:appinfo>
							    </xs:annotation>
						    </xs:element>
						    <xs:element name="entre201e300Pessoas" type="xs:boolean">
							    <xs:annotation>
								    <xs:appinfo>
									    <xhtml:label>Entre 401 e 500 Pessoas</xhtml:label>
								    </xs:appinfo>
							    </xs:annotation>
						    </xs:element>
						    <xs:element name="acimaDe501" type="xs:boolean">
							    <xs:annotation>
								    <xs:appinfo>
									    <xhtml:label>Acima de 501</xhtml:label>
								    </xs:appinfo>
							    </xs:annotation>
						    </xs:element>
						    
		            </xs:sequence>
	            </xs:complexType>
            </xs:element>               

			            <xs:element name="sintese" minOccurs="0" type="xs:string">
	                      <xs:annotation>
		                    <xs:appinfo>
			                 <xhtml:label>6.0 Síntese</xhtml:label>
		                   </xs:appinfo>
	                     </xs:annotation>
                </xs:element>




		</xs:sequence>
	</xs:complexType>

</xs:schema>
