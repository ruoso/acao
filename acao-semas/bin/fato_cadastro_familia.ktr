    <sql>CREATE  MATERIALIZED  VIEW TB_FATO_CADASTRO_FAMILIA AS
select
COD_LOCAL_DOMIC_FAM,
COD_ESPEC_DOMIC_FAM,
COD_MATERIAL_PISO_FAM,
COD_MATERIAL_DOMIC_FAM,
COD_AGUA_CANALIZADA_FAM,
COD_ABASTE_AGUA_DOMIC_FAM,
COD_BANHEIRO_DOMIC_FAM,
COD_ESCOA_SANIT_DOMIC_FAM,
COD_DEST_LIXO_DOMIC_FAM,
COD_ILUMIN_DOMIC_FAM,
COD_CALC_DOMIC_FAM,
QTD_COMODOS_DOMIC_FAM,
QTD_COMODOS_DORM_FAM,
decode(b.DESCRICAOP1,null,&apos;-1&apos;,b.COD_BAIRRO) as COD_BAIRRO ,
nvl(SER,&apos;0&apos;) as SER,
CIDADE_NOME,
QUANT_PESSOAS,
DAT_CAD_FAMILIA,
qtdfamilias
from V_FATO_CADASTRO_FAM_NEW c, TB_BAIRRO_SEMAS b
where b.DESCRICAOP1(+) = c.BAIRRO_NOME</sql>
