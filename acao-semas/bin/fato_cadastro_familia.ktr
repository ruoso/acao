    <sql>CREATE OR REPLACE VIEW V_FATO_CADASTRO_FAM_NEW AS
select
COD_LOCAL_DOMIC_FAM,
COD_ESPEC_DOMIC_FAM,
COD_MATERIAL_PISO_FAM,
COD_MATERIAL_DOMIC_FAM,
COD_AGUA_CANALIZADA_FAM,
COD_ABASTE_AGUA_DOMIC_FAM,
COD_BANHEIRO_DOMIC_FAM,
COD_ESCOA_SANIT_DOMIC_FAM,
COD_DEST_LIXO_DOMIC_FAM,
COD_ILUMIN_DOMIC_FAM,
COD_CALC_DOMIC_FAM,
QTD_COMODOS_DOMIC_FAM,
QTD_COMODOS_DORM_FAM,
BAIRRO_NOME,
SER,
CIDADE_NOME,
QUANT_PESSOAS,
DAT_CAD_FAMILIA,
count(1) qtdfamilias
from
(
   SELECT
   replace(COD_LOCAL_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_LOCAL_DOMIC_FAM,
   replace(COD_ESPEC_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_ESPEC_DOMIC_FAM,
   replace(COD_MATERIAL_PISO_FAM,&apos; &apos;,&apos;-1&apos;) as COD_MATERIAL_PISO_FAM,
   replace(COD_MATERIAL_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_MATERIAL_DOMIC_FAM,
   replace(COD_AGUA_CANALIZADA_FAM,&apos; &apos;,&apos;-1&apos;) as COD_AGUA_CANALIZADA_FAM,
   replace(COD_ABASTE_AGUA_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_ABASTE_AGUA_DOMIC_FAM,
   replace(COD_BANHEIRO_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_BANHEIRO_DOMIC_FAM,
   replace(COD_ESCOA_SANIT_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_ESCOA_SANIT_DOMIC_FAM,
   replace(COD_DEST_LIXO_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_DEST_LIXO_DOMIC_FAM,
   replace(COD_ILUMIN_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_ILUMIN_DOMIC_FAM,
   replace(COD_CALC_DOMIC_FAM,&apos; &apos;,&apos;-1&apos;) as COD_CALC_DOMIC_FAM,
   replace(QTD_COMODOS_DOMIC_FAM,&apos;  &apos;,&apos;-1&apos;) as QTD_COMODOS_DOMIC_FAM,
   replace(QTD_COMODOS_DORM_FAM,&apos;  &apos;,&apos;-1&apos;) as QTD_COMODOS_DORM_FAM,
   (select c1.CBAIRRO_NOME from CEP_CE c1 where nvl(substr(crt.NUM_CEP_LOGRA_FAM,1,5),&apos;60000&apos;) = substr(c1.CLOG_CEP,1,5) and rownum = 1) BAIRRO_NOME,
   (select c1.CSER_ID from CEP_CE c1 where nvl(substr(crt.NUM_CEP_LOGRA_FAM,1,5),&apos;60000&apos;) = substr(c1.CLOG_CEP,1,5) and rownum = 1) SER,
   (select c1.CCIDADE_NOME from CEP_CE c1 where nvl(substr(crt.NUM_CEP_LOGRA_FAM,1,5),&apos;60000&apos;) = substr(c1.CLOG_CEP,1,5) and rownum = 1) CIDADE_NOME,
   replace(CRT.DAT_CAD_FAMILIA,&apos; &apos;,&apos;-1&apos;) as DAT_CAD_FAMILIA,
   pes.QUANT as QUANT_PESSOAS
   FROM V_CARACTERIZACAO_DOMICILIO c,
   V_IDENTIFICACAO_CONTROLE crt,   
   V_QUANT_PESSOA_FAM pes
   where crt.COD_FAMILIAR_FAM = c.COD_FAMILIAR_FAM
   and c.COD_FAMILIAR_FAM = pes.COD_FAMILIAR_FAM_1
   
   and exists
   (
      select
      1
      from cep_ce x
      where nvl(substr(crt.NUM_CEP_LOGRA_FAM,1,5),&apos;60000&apos;) = substr(x.CLOG_CEP,1,5)
   )
)
GROUP BY COD_LOCAL_DOMIC_FAM,
COD_ESPEC_DOMIC_FAM,
COD_MATERIAL_PISO_FAM,
COD_MATERIAL_DOMIC_FAM,
COD_AGUA_CANALIZADA_FAM,
COD_ABASTE_AGUA_DOMIC_FAM,
COD_BANHEIRO_DOMIC_FAM,
COD_ESCOA_SANIT_DOMIC_FAM,
COD_DEST_LIXO_DOMIC_FAM,
COD_ILUMIN_DOMIC_FAM,
COD_CALC_DOMIC_FAM,
QTD_COMODOS_DOMIC_FAM,
QTD_COMODOS_DORM_FAM,
BAIRRO_NOME,
SER,
CIDADE_NOME,
QUANT_PESSOAS,
DAT_CAD_FAMILIA
</sql>
