<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	xmlns="http://schemas.fortaleza.ce.gov.br/acao/gmfpronasci-protejoInstrumentalHabilidades.xsd"
	xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
	xmlns:idx="http://schemas.fortaleza.ce.gov.br/acao/indexhint.xsd"
	targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/gmfpronasci-protejoInstrumentalHabilidades.xsd"
	elementFormDefault="qualified">

	<xs:element name="formProtejoInstrumentalHabilidades" type="formProtejoInstrumentalHabilidades">
		<xs:annotation>
			<xs:appinfo>
				<xhtml:label>INSTRUMENTAL PARA IDENTIFICAÇÃO DE INTERESSES E
					DISPONIBILIDADE PARA ATIVIDADES DO PROTEJO</xhtml:label>
				<class:classificacoes>
					<class:classificacao>cn=Segurança Pública</class:classificacao>
				</class:classificacoes>
				<idx:index>
				    <idx:hint key="protejo.numeroInscricao" xpath="formProtejoInstrumentalHabilidades/dadosInscricao/numeroInscricao" />
				    <idx:hint key="protejo.nomejovem" xpath="formProtejoInstrumentalHabilidades/dadosJovem/nomeJovem" />
				    <idx:hint key="protejo.idadejovem" xpath="formProtejoInstrumentalHabilidades/dadosJovem/idadeJovem" />
				    <idx:hint key="protejo.redeSocial" xpath="formProtejoInstrumentalHabilidades/dadosJovem/redeSocial" />
				    <idx:hint key="protejo.nucleo" xpath="formProtejoInstrumentalHabilidades/dadosInscricao/nomeEntrevistador" />
				    <idx:hint key="protejo.nomeEntrevistador" xpath="formProtejoInstrumentalHabilidades/dadosInscricao/nomeEntrevistador" />
				</idx:index>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="formProtejoInstrumentalHabilidades">
		<xs:sequence>

			<xs:element name="dadosInscricao">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>DADOS INSCRIÇÃO</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="numeroInscricao" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Número de inscrição</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="dadosJovem">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>DADOS JOVEM</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="nomeJovem" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Nome</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="telefoneJovem" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Telefone</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="idadeJovem" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Idade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="redeSocial" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Rede social/orkut</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="racaEtnia">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Raça/etnia</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Branco(caucasiano)" />
									<xs:enumeration value="Negro" />
									<xs:enumeration value="Pardo" />
									<xs:enumeration value="Amarelo" />
									<xs:enumeration value="Indígena" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="nucleo" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Núcleo</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="turno">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Turno</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Manhã" />
									<xs:enumeration value="Tarde" />
									<xs:enumeration value="Noite" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="dadosTrabalho">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1. TRABALHO</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="estaTrabalhando">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Esta trabalhando atualmente?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
									<xs:enumeration value="Não, mas já trabalhei" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="atividadeExerceu" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Qual atividade você exerce ou exerceu?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="carteiraAssinada">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Trabalha com carteira assinada.</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ondeTrabalha" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Onde trabalha?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="dataAssinaturaCarteira" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data de assinatura da carteira?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="trabalhaInformal">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Trabalho informal</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="ondeTrabalha2" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Onde trabalha?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="empreendimentoInformal">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Empreendimento individual ou coletivo.</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="tipo" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Qual tipo?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="horarioTrabalho">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quais os dias e o horário de trabalho?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Semana inteira" />
									<xs:enumeration value="Finais de semana" />
									<xs:enumeration value="Alguns dias da semana" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="diasDeTrabalho" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quais dias?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="horario">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Turno</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Manhã" />
									<xs:enumeration value="Tarde" />
									<xs:enumeration value="Noite" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="remuneracao" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Qual sua remuneração?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="dadosEscolares">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>2. ESCOLA</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="estudando">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Esta estudando atualmente?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="horario">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Horário</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Manhã" />
									<xs:enumeration value="Tarde" />
									<xs:enumeration value="Noite" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="nomeEscola" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Qual o nome da escola?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="serieQueParou" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Parou em qual série? </xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="voltarEstudar">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tem interesse em retornar para a escola:
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="dadosDocumentacao">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>3. DOCUMENTAÇÃO</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>

						<xs:element name="documentos">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Marque com X a documentacao que possue</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="rg" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>RG</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="numeroRG" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Número RG </xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="dataEmissao" type="xs:date"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Data da emissão</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="certidaoNascimento" type="xs:boolean"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Certidão de nascimento</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="numeroCertidaoNascimento"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Número da certidão nascimento </xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="carteiraTrabalho" type="xs:boolean"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Carteira de trabalho</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="numeroCarteiraTrabalho" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Número carteira de trabalho </xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="tituloEleitor" type="xs:boolean"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Titulo de eleitor</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="numeroTitulo" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Número titulo de eleitor </xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="alistamento" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Já se alistou no serviço militar?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>


						<xs:element name="voltarEstudar" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Possui certidão de reservista? </xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="nemeroReservista" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Número reservista </xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<!-- REFERÊNCIAS PESSOAIS -->

			<!-- DADOS DA CONFIGURAÇÃO FAMILIAR -->

			<xs:element name="trabalho">

				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>4. TEMPO LIVRE</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>

					<xs:sequence>
						<xs:element name="disponibilidade">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label> Marque com um X os seus horários disponíveis:
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="disponibilidadeSegunda">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Segunda</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="manha" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Manhã</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="tarde" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Tarde</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="noite" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Noite</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>

									<xs:element name="disponibilidadeTerca">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Terça</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="manha" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Manhã</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="tarde" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Tarde</xhtml:label>
														</xs:appinfo>
