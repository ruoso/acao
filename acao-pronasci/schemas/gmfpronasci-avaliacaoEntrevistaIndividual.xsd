											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="resposta10">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>"Olho por olho..."</xhtml:label>
								</xs:appinfo>
