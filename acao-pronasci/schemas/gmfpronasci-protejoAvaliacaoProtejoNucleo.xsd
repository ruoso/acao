						<xs:element name="pontualidade">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>A) Pontualidade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Ótimo" />
									<xs:enumeration value="Bom" />
									<xs:enumeration value="Ruim" />
								</xs:restriction>
							</xs:simpleType>
