								<xs:restriction base="xs:string">
									<xs:enumeration value="Ótimo" />
									<xs:enumeration value="Bom" />
									<xs:enumeration value="Ruim" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="comunicaoGrupo">
							<xs:annotation>
								<xs:appinfo>
